{% extends 'base.html.twig' %}

{% block title %}ParteDiario index{% endblock %}

{% block body %}
    <h1>ParteDiario index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for parte_diario in parte_diarios %}
            <tr>
                <td>{{ parte_diario.id }}</td>
                <td>{{ parte_diario.fecha ? parte_diario.fecha|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_parte_diario_show', {'id': parte_diario.id}) }}">show</a>
                    <a href="{{ path('app_parte_diario_edit', {'id': parte_diario.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_parte_diario_new') }}">Create new</a>
{% endblock %}
